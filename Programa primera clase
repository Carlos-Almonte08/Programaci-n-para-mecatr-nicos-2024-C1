#include <stdio.h>
#include <stdlib.h>

int main()
{
    int edad = 0;

    printf("\nEscribe tu edad");
    scanf ("%d",&edad);
    if(edad > 0 && edad <130)
    {
        if(edad < 18)
        {
            printf("\nEres menor de edad\n");

        }
        else
        {
            printf("n\Eres mayor edad\n");
        }

    }
    else
    {
        printf("\nEres un jablador\n");
    }

    return 0;
}
